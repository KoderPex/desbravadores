	<queryString>
		<![CDATA[SELECT DISTINCT 1 AS ORD, ta.TP_ITEM, ta.CD_ITEM_INTERNO, ta.DS_ITEM AS X, ca.NM, ta.DS_ITEM
FROM CAD_COMPRAS_PESSOA ccp
INNER JOIN CON_ATIVOS ca ON (ca.ID = ccp.ID_CAD_PESSOA)
INNER JOIN TAB_APRENDIZADO ta ON (ta.ID = ccp.ID_TAB_APREND)
WHERE ta.ID_DSA IS NOT NULL
  AND ta.TP_ITEM = 'CL'
UNION ALL

SELECT DISTINCT 2 AS ORD, ta.TP_ITEM, ta.DS_ITEM AS X, ta.CD_ITEM_INTERNO, ca.NM, ta.DS_ITEM
FROM CAD_COMPRAS_PESSOA ccp
INNER JOIN CON_ATIVOS ca ON (ca.ID = ccp.ID_CAD_PESSOA)
INNER JOIN TAB_APRENDIZADO ta ON (ta.ID = ccp.ID_TAB_APREND)
WHERE ta.ID_DSA IS NOT NULL
  AND ta.TP_ITEM = 'ES'
  AND SUBSTR(ta.CD_ITEM_INTERNO,1,2) <> 'ME'

UNION ALL

SELECT DISTINCT 3 AS ORD, ta.TP_ITEM, ta.DS_ITEM AS X, ta.CD_ITEM_INTERNO, ca.NM, ta.DS_ITEM
FROM CAD_COMPRAS_PESSOA ccp
INNER JOIN CON_ATIVOS ca ON (ca.ID = ccp.ID_CAD_PESSOA)
INNER JOIN TAB_APRENDIZADO ta ON (ta.ID = ccp.ID_TAB_APREND)
WHERE ta.ID_DSA IS NOT NULL
  AND ta.TP_ITEM = 'ES'
  AND SUBSTR(ta.CD_ITEM_INTERNO,1,2) = 'ME'

ORDER BY 1, 2, 3, 4, 5]]>
