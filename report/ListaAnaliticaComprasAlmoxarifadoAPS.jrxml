    ? "GAVETA NÃO DEFINIDA"
    : ($F{NR_GAVETA_APS} == 9999
        ? "RESUMO CONSOLIDADO POR TIPO"
        : "GAVETA ".concat( $F{NR_GAVETA_APS}.toString() )
